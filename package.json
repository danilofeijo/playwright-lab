    "test:dev": "ENV=dev npx playwright test e2e/",
    "test:stg": "ENV=stg npx playwright test e2e/",
    "check:eslint": "npx eslint .",
    "check:prettier": "npx prettier --check .",
    "check:all": "npm run check:eslint && npm run check:prettier && npm run test:dev"
